{
    "$jason": {
        "head": {
            "title": "Web Container Agent",
            "actions": {
                "open_external_browser": {
                    "type": "javascript",
                    "options": {
                        "script": "window.open($jason.url, '_system')"
                    }
                }
            }
        },
        "body": {
            "background": {
                "type": "html",
                "url": "https://orka.fitness/sign_in_fasttrack?native=yes",
                "style": {
                    "background": "#000000",
                    "progress" : "rgba(255,255,255,0.15)"
                },
                "action": {
                    "trigger": "open_external_browser"
                }
            }
        }
    }
}
